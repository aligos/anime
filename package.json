{
  "name": "meteor-mantra-kickstarter",
  "version": "0.3.1",
  "scripts": {
    "lint": "eslint ./lib ./client ./server --ext .js",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9001 -s ./public"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.7.0",
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.2.0",
    "eslint": "1.10.x",
    "eslint-plugin-react": "3.15.x",
    "jsdom": "^8.0.4",
    "mocha": "2.x.x",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.0.0",
    "sinon": "1.17.x",
    "style-loader": "^0.13.1"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "formsy-react": "0.17.0",
    "formsy-react-components": "0.6.6",
    "lodash": "4.6.1",
    "mantra-core": "^1.5.0",
    "moment": "2.11.2",
    "react": "^15.0.0",
    "react-bootstrap": "0.28.3",
    "react-dom": "^15.0.0",
    "react-iframe": "0.0.4",
    "react-mounter": "^1.2.0",
    "react-select": "0.9.1"
  },
  "private": true
}
